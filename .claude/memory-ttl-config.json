{
  "namespaceConfiguration": {
    "core-architecture": {
      "ttl": null,
      "persist": true,
      "description": "Permanent architectural decisions and system design",
      "compression": false,
      "encryption": false
    },
    "supervision-trees": {
      "ttl": null,
      "persist": true,
      "description": "Supervision strategies and agent hierarchy",
      "compression": false,
      "versioning": true
    },
    "agent-domains": {
      "ttl": "7d",
      "persist": true,
      "description": "Agent state snapshots and domain knowledge",
      "compression": true,
      "compressionLevel": 3,
      "autoArchive": true
    },
    "transport-layer": {
      "ttl": "1h",
      "persist": false,
      "description": "NATS message patterns and routing cache",
      "cache": "hot",
      "evictionPolicy": "LRU",
      "maxEntries": 10000
    },
    "data-management": {
      "ttl": "24h",
      "persist": true,
      "description": "Database operations and query patterns",
      "index": true,
      "indexType": "btree",
      "partitioning": "daily"
    },
    "monitoring": {
      "ttl": "24h",
      "persist": false,
      "description": "Telemetry data and performance metrics",
      "aggregate": true,
      "aggregationInterval": "5m",
      "retention": "7d"
    },
    "operations": {
      "ttl": "30d",
      "persist": true,
      "description": "Deployment history and operational decisions",
      "archive": true,
      "archiveFormat": "compressed_json",
      "searchable": true
    },
    "security": {
      "ttl": null,
      "persist": true,
      "description": "Security policies and access controls",
      "encrypt": true,
      "encryptionAlgorithm": "AES-256-GCM",
      "auditLog": true
    },
    "testing": {
      "ttl": "7d",
      "persist": false,
      "description": "Test results and validation outcomes",
      "cleanup": true,
      "cleanupPolicy": "on_success",
      "keepFailures": "30d"
    },
    "collaboration": {
      "ttl": "3d",
      "persist": true,
      "description": "Inter-agent communication and coordination",
      "sync": true,
      "syncInterval": "1m",
      "conflictResolution": "last_write_wins"
    }
  },
  "globalSettings": {
    "defaultTTL": "24h",
    "maxMemoryUsage": "1GB",
    "compressionThreshold": "10KB",
    "maintenanceWindow": "02:00-03:00",
    "backupEnabled": true,
    "backupInterval": "6h"
  }
}