{
  "mcpServers": {
    "aws-core-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "--interactive",
        "--network", "mistersmith-mcp-network",
        "--env-file", "./docker/aws-mcp-server/.env",
        "--volume", "${HOME}/.aws:/home/mcpuser/.aws:ro",
        "--name", "aws-mcp-server-instance",
        "aws-mcp-server:latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    }
  }
}